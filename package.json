{
  "name": "fe",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "upgrade": "npx npm-check-updates &&  npx npm-check-updates -u",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "relay": "relay-compiler",
    "all": "node run_all.js",
    "postinstall": "pnpm all",
    "clean-fbts": "rm .enum_manifest.json .src_manifest.json .source_strings.json src/translatedFbts.json .test_*.json 2&> /dev/null || exit 0",
    "collect-fbts": "fbt-collect --fbt-common-path ./common_strings.json --pretty --manifest < .src_manifest.json > .source_strings.json",
    "manifest": "fbt-manifest --src src",
    "translate-fbts-single-file": "fbt-translate --jenkins --stdin < translation_input.json > src/translatedFbts.json",
    "translate-fbts": "fbt-translate --translations translations/*.json --jenkins > src/translatedFbts.json",
    "watch:fbts": "yarn watch 'yarn clean-fbts && yarn all' ./src/example ../packages/babel-plugin-fbt/dist/ --ignoreDotFiles  --wait=1"
  },
  "dependencies": {
    "@stylexjs/stylex": "0.3.0",
    "@tanstack/react-router": "0.0.1-beta.234",
    "fbjs": "^3.0.5",
    "react": "git://github.com/tienlx97/react-experimental.git#www",
    "react-dom": "git://github.com/tienlx97/react-dom-experimental.git#www",
    "react-relay": "^16.0.0",
    "babel-plugin-fbt": "1.0.0",
    "babel-plugin-fbt-runtime": "1.0.0",
    "fbt": "1.0.0",
    "fbt-generate-translations": "0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "7.22.19",
    "@babel/preset-react": "^7.18.6",
    "@stylexjs/babel-plugin": "0.3.0",
    "@stylexjs/eslint-plugin": "^0.3.0",
    "@stylexjs/webpack-plugin": "^0.3.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-relay": "^16.0.0",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.53.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "graphql": "^16.8.1",
    "html-webpack-plugin": "^5.5.0",
    "postcss-loader": "7.3.3",
    "prettier": "^2.8.3",
    "react-hot-loader": "^4.13.1",
    "relay-compiler": "^16.0.0",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1"
  }
}
